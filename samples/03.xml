<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html b:version='2' class='v2'
  xmlns='http://www.w3.org/1999/xhtml' 
  xmlns:b='http://www.google.com/2005/gml/b' 
  xmlns:data='http://www.google.com/2005/gml/data' 
  xmlns:expr='http://www.google.com/2005/gml/expr'>
  <head>
    <title>
      <data:blog.pageTitle/>
    </title>
    
    <!-- Bắt đầu viết Css cho web -->
    <b:skin>
      <![CDATA[
        .ui-wrapper {
          width: 800px;
          margin: 0 auto;
        }
        
        .ui-header {
          width: 100%;
          min-height: 90px;
          background: #4d90fe;
          overflow: hidden;
          display: inline-block;      
        }
        
        .ui-main-wrapper {
          width: 70%;
          float:left;
          min-height: 300px;
          background: #f0c36d;
        }
        
        .ui-sidebar-wrapper {
          width: 30%;
          float:right;
          min-height: 300px;
          background: #f9edbe;
        }
        
      ]]>
    </b:skin>
  
  </head>
  
  <!-- Bắt đầu phần hiển thị trên web -->
  <body id='layout'>
    <div class='ui-wrapper'>
      <div class='ui-header'>
          <div class='ui-header-left'>
            <b:section id='HeaderLogo' class='ui-header-logo' showaddelement='yes'>
              <b:widget id='Header1' locked='false' type='Header' visible='true'>
                <b:widget-settings>
                  <b:widget-setting name='displayUrl'>data:image/png;base64,</b:widget-setting>
                  <b:widget-setting name='displayHeight'>460</b:widget-setting>
                  <b:widget-setting name='sectionWidth'>792</b:widget-setting>
                  <b:widget-setting name='useImage'>true</b:widget-setting>
                  <b:widget-setting name='shrinkToFit'>false</b:widget-setting>
                  <b:widget-setting name='imagePlacement'>REPLACE</b:widget-setting>
                  <b:widget-setting name='displayWidth'>460</b:widget-setting>
                </b:widget-settings>                
              </b:widget>
            </b:section>
          </div>
          <div class='ui-header-right'>
            <b:section id='HeaderText' class='ui-header-text' showaddelement='yes'>
              <b:widget id='Header2' locked='false' type='Header' visible='true'/>
            </b:section>
          </div>
        <b:template-skin>
          <![CDATA[
            #layout .ui-header-left {
              overflow: hidden;
              display: inline-block;      
              width: 30%;
              float: left;
            }
        
            #layout .ui-header-left img {
              height: 100%;
              width:auto;
            }
        
            #layout .ui-header-right {
              overflow: hidden;
              display: inline-block;        
              width: 70%;
              float: right;
            }
          ]]>
        </b:template-skin>
      </div>
      <div class='ui-main-wrapper'>
        <b:section class='ui-main-articles' id='MainPost' showaddelement='yes'>
          <b:widget id='Blog1' locked='false' type='Blog'/>
        </b:section>
      </div>
      <div class='ui-sidebar-wrapper'>
        <b:section class='sidebar' id='Sidebar' showaddelement='yes'/>
      </div>
    </div>
  </body>
  <!-- Kết thúc phần hiển thị trên web -->

</html>